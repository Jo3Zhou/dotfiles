# Easy report
alias cp='cp -iv'
alias mv='mv -iv'
alias ln='ln -iv'
alias mkdir='mkdir -v'

# APP alias
alias v='nvim'
alias g='git'
alias cs='cowsay'
alias nf='neofetch'

# Bookmarks
alias work='cd ~/work && ls -lA'
alias dl='cd ~/Downloads/ && ls -lA'

# Typos
alias :q='exit'
alias help='man'
alias quit='exit'
